<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mohit.codemania">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/code"
        android:label="@string/app_name"
        android:roundIcon="@drawable/code"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".home"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Signup"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Browser"
            android:label="@string/title_activity_tabbed"
            android:parentActivityName=".Tabbed"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Tabbed"
            android:label="@string/title_activity_tabbed"
            android:parentActivityName=".Tabbed"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Userdata"
            android:label="@string/title_activity_userdata"
            android:parentActivityName=".Userdata"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Calendar"
            android:label="@string/title_activity_Calendar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SuggestMain"
            android:label="@string/title_activity_problemset"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SearchUser"
            android:label="@string/title_activity_search_user"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Contact" />
        <activity android:name=".About"></activity>
    </application>

</manifest>